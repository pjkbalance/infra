[client]
port=3306                             # 客户端连接端口
socket=/var/run/mysqld/mysqld.sock   # 客户端socket文件路径

[mysqld]
user=mysql                           # MySQL服务运行用户
port=3306                            # 服务端口
bind-address=0.0.0.0                 # 监听所有IP地址
datadir=/var/lib/mysql               # 数据文件存储目录
socket=/var/run/mysqld/mysqld.sock   # 服务端socket文件路径
symbolic-links=0                     # 禁止符号链接，提升安全性
sql_mode=STRICT_TRANS_TABLES,NO_ENGINE_SUBSTITUTION  # SQL模式设置
max_connections=200                  # 最大连接数
character-set-server=utf8mb4         # 服务端默认字符集
collation-server=utf8mb4_unicode_ci  # 服务端默认字符序
default_authentication_plugin=mysql_native_password   # 默认认证插件

[mysql]
default-character-set=utf8mb4        # mysql客户端默认字符集